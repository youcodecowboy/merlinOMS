{
  "name": "@app/api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "test:integration": "pnpm setup:test-db && NODE_ENV=test jest -c jest.integration.config.ts",
    "test:integration:verbose": "pnpm setup:test-db && NODE_ENV=test jest -c jest.integration.config.ts --verbose",
    "setup:test-db": "bash scripts/setup-test-db.sh",
    "test:bin": "NODE_ENV=test jest -c jest.config.ts src/services/bin/__tests__/bin.service.test.ts",
    "test:bin:watch": "NODE_ENV=test jest -c jest.config.ts src/services/bin/__tests__/bin.service.test.ts --watch",
    "test:bin:coverage": "NODE_ENV=test jest -c jest.config.ts src/services/bin/__tests__/bin.service.test.ts --coverage",
    "test:order": "NODE_ENV=test jest -c jest.config.ts src/services/order/__tests__/order-fulfillment.service.test.ts",
    "test:order:watch": "NODE_ENV=test jest -c jest.config.ts src/services/order/__tests__/order-fulfillment.service.test.ts --watch",
    "test:order:routes": "NODE_ENV=test jest -c jest.config.ts src/routes/__tests__/order.routes.test.ts",
    "test:order:routes:watch": "NODE_ENV=test jest -c jest.config.ts src/routes/__tests__/order.routes.test.ts --watch"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "helmet": "^7.2.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/ioredis-mock": "^8.2.5",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.19.67",
    "@types/supertest": "^6.0.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.16.0",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
